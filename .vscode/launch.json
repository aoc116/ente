{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Local",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "program": "lib/main.dart",
      "args": [
        "--dart-define",
        "endpoint=http://localhost:8080"
      ]
    },
    {
      "name": "Android Dev",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--dart-define",
        "endpoint=http://192.168.1.3:8080",
        "--flavor",
        "independent"
      ]
    },
    {
      "name": "iOS Dev",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--dart-define",
        "endpoint=http://192.168.1.30:8080"
      ]
    },
    {
      "name": "iOS Prod",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--target",
        "lib/main.dart"
      ]
    },
    {
      "name": "Android Prod",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--target",
        "lib/main.dart",
        "--flavor",
        "independent"
      ]
    },
  ]
}